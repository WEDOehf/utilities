{
  "name": "wedo/utilities",
  "description": "Utility classes",
  "license": ["MIT"],
  "require": {
    "php": "^8.1",
    "nette/utils": "^3.0|^4.0",
    "nette/caching": "^3.0",
    "contributte/aop":"^3.0",
    "nette/security": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.0",
    "dg/bypass-finals": "^1.2",
    "contributte/qa": "^0.3",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-dibi": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "psr/log": "^1.1",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Wedo\\Utilities\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wedo\\Utilities\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
